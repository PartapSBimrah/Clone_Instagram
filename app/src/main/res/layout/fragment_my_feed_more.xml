<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".src.main.home.feedMore.MyFeedMoreFragment">

    <TextView
        android:layout_width="40dp"
        android:layout_height="5dp"
        android:layout_gravity="center"
        android:layout_marginVertical="10dp"
        android:background="@drawable/bottom_sheet_fragment_background_top" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:paddingHorizontal="15dp"
        android:textSize="15sp"
        android:text="다른 앱에 게시..."
        android:textColor="@color/black" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:paddingHorizontal="15dp"
        android:textSize="15sp"
        android:text="링크 복사"
        android:textColor="@color/black"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:paddingHorizontal="15dp"
        android:textSize="15sp"
        android:text="공유하기..."
        android:textColor="@color/black" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:paddingHorizontal="15dp"
        android:textSize="15sp"
        android:text="보관"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/my_feed_more_delete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:paddingHorizontal="15dp"
        android:textSize="15sp"
        android:text="삭제"
        android:textColor="@color/black"/>

    <TextView
        android:id="@+id/my_feed_more_modify"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:paddingHorizontal="15dp"
        android:textSize="15sp"
        android:text="수정"
        android:textColor="@color/black" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:paddingVertical="12dp"
        android:paddingHorizontal="15dp"
        android:textSize="15sp"
        android:text="댓글 기능 해제"
        android:textColor="@color/black" />

</LinearLayout>